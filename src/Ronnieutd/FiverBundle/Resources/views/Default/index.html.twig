<h2><b>High Fiver App</b></h2>

<div align="center">

    <h1>Top 5 Players</h1>

    <table width="300">
        {% for player in topFivePlayers %}
        <tr><td># {{ player.rank }} -  <a href="{{
                path('player', { playerId:player.playerId,  sportId:sportId })
                }}">{{ player.name | trim }}</a></td></tr>
        {% endfor %}
    </table>
</div>

